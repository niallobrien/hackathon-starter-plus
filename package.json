{
  "name": "hackathon-starter-plus",
  "version": "4.3.0",
  "description": "A boilerplate for Node.js web applications",
  "repository": {
    "type": "git",
    "url": "https://github.com/niallobrien/hackathon-starter-plus"
  },
  "author": "Niall O'Brien",
  "license": "MIT",
  "scripts": {
    "start": "node app.js",
    "dev": "cross-env NODE_ENV=development webpack --progress --hide-modules",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot",
    "production": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "prod": "npm run production",
    "test": "mocha --reporter spec",
    "eslint": "eslint **/*.js"
  },
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "chalk": "^2.1.0",
    "cheerio": "^1.0.0-rc.2",
    "chunk-manifest-webpack-plugin": "^0.1.0",
    "clockwork": "^0.1.4",
    "compression": "^1.7.0",
    "connect-mongo": "^2.0.0",
    "cross-env": "^3.2.4",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.15.4",
    "express-flash": "^0.0.2",
    "express-rev": "^0.1.2",
    "express-session": "^1.15.5",
    "express-status-monitor": "^1.0.0",
    "express-validator": "^3.2.1",
    "fbgraph": "^1.4.1",
    "font-awesome": "^4.7.0",
    "github": "^9.2.0",
    "instagram-node": "^0.5.8",
    "jquery": "^3.1.1",
    "laravel-mix": "^0.8.8",
    "lastfm": "^0.9.2",
    "lob": "^4.1.1",
    "lusca": "^1.5.1",
    "lodash": "^4.16.6",
    "mongoose": "^4.11.7",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "node-foursquare": "^0.3.0",
    "node-linkedin": "^0.5.6",
    "nodemailer": "^4.0.1",
    "passport": "0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-instagram": "^1.0.0",
    "passport-linkedin-oauth2": "^1.5.0",
    "passport-local": "^1.0.0",
    "passport-oauth": "^1.0.0",
    "passport-openid": "^0.4.0",
    "passport-twitter": "^1.0.4",
    "paypal-rest-sdk": "^1.7.1",
    "pug": "^2.0.0-rc.3",
    "request": "^2.81.0",
    "simple-pjax": "^0.3.0",
    "socket.io": "^1.7.2",
    "stripe": "^4.24.0",
    "tumblr.js": "^1.1.1",
    "twilio": "^3.6.3",
    "twit": "^2.2.9",
    "validator": "^8.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "bootstrap-sass": "^3.3.7",
    "chai": "^4.1.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-chai-friendly": "^0.4.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.5.0",
    "sass-loader": "^6.0.5",
    "sinon": "^3.2.1",
    "sinon-mongoose": "^2.0.2",
    "supertest": "^3.0.0",
    "webpack": "^2.2.1",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0,
      "no-unused-expressions": 0,
      "chai-friendly/no-unused-expressions": 2
    },
    "env": {
      "jasmine": true,
      "mocha": true,
      "node": true
    },
    "plugins": [
      "chai-friendly"
    ]
  }
}
